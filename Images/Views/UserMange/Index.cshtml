@model IEnumerable<Images.AspNetUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAll.cshtml";
}
<center style="margin-top:20px">
    <h2>จัดการบัญชีผู้ใช้</h2>
</center>
<div style="padding-left: 10%; padding-right: 10%;margin-top:5px;justify-content:center">
    <table class="table table-striped" style="align-content:center">
        <tr>
            <th>
                Profile
            </th>  
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
               @* @Html.DisplayNameFor(model => model.FirstName)*@
                <label>UserName</label>
            </th>

            <th>
               @* @Html.DisplayNameFor(model => model.LastName)*@

            </th>
          
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
    <tr>
        <td>
            @*  @Html.DisplayFor(modelItem => item.pic_url)*@
            @*<img src=@Url.Content("~/Content/images/"+(item.LastName)) style="width: 100px;height: auto," />*@
            <img src=@Url.Content("~/Content/images/Profile/"+(item.LastName)) style="width: 100px;height: auto," />
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>


        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "btn btn-outline-primary" })
            
            @Html.ActionLink("Delete", "Delete", new { id = item.Id, @class = "btn btn-danger" }, new { @class = "btn btn-danger " })
        </td>
    </tr>
        }

    </table>
</div>